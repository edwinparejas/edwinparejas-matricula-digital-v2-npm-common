{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"allowJs": true,
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"sourceMap": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
